{
  "name": "audio-video-transcriber-server",
  "version": "1.0.0",
  "description": "backed ui for audio/video transcription server using Sherpa-ONNX",
  "main": "server.js",
  "scripts": {
    "start": "LD_LIBRARY_PATH=$PWD/node_modules/sherpa-onnx-linux-arm64:$LD_LIBRARY_PATH node --expose-gc server.js",
    "start:debug": "LOG_LEVEL=DEBUG LD_LIBRARY_PATH=$PWD/node_modules/sherpa-onnx-linux-arm64:$LD_LIBRARY_PATH node --expose-gc server.js",
    "dev": "LOG_LEVEL=DEBUG LD_LIBRARY_PATH=$PWD/node_modules/sherpa-onnx-linux-arm64:$LD_LIBRARY_PATH nodemon --expose-gc server.js",
    "lint": "eslint --fix *.js",
    "lint:html": "eslint --fix index.html"
  },
  "keywords": [
    "transcription",
    "translate",
    "audio",
    "video",
    "sherpa-onnx",
    "subtitle",
    "websocket",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "express": "^5.1.0",
    "lru-cache": "^11.1.0",
    "multer": "^2.0.2",
    "sherpa-onnx-linux-arm64": "^1.12.9",
    "sherpa-onnx-node": "^1.12.9",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-regexp": "^2.10.0",
    "globals": "^16.3.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "type": "module"
}
